{
  "name": "SpaceSuit",
  "version": "0.0.1",
  "description": "An extension to allow Ledger devices to use Ethereum Dapps",
  "manifest_version": 2,
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "run_at": "document_start",
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    "inpage.js"
  ],
  "options_page": "options.html"
}
