{
  "name": "spacesuit",
  "version": "0.2.5",
  "description": "A Chrome extension to enable Ledger devices to use Etherem Dapps",
  "main": "index.js",
  "scripts": {
    "test": "mocha --require babel-register",
    "build": "parcel build --no-minify content.js && parcel build --no-minify inpage.js && parcel build --no-minify --public-url / options.html && parcel build --no-minify --public-url / --out-dir integration-test/build integration-test/index.html && cp -R manifest.json icons/ dist/",
    "clean": "rm dist.zip; rm -rd dist; rm -rd integration-test/build; true",
    "dist": "npm run-script clean && npm run-script build && cd dist && zip -r ../dist.zip ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamespic/SpaceSuit.git"
  },
  "keywords": [
    "ethereum",
    "dapps",
    "web3",
    "chrome-extension",
    "ledger"
  ],
  "author": "James Pickering",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jamespic/SpaceSuit/issues"
  },
  "homepage": "https://github.com/jamespic/SpaceSuit#readme",
  "dependencies": {
    "@ledgerhq/hw-app-eth": "../ledgerjs/packages/hw-app-eth",
    "@ledgerhq/hw-transport": "../ledgerjs/packages/hw-transport",
    "@ledgerhq/hw-transport-u2f": "../ledgerjs/packages/hw-transport-u2f",
    "@ledgerhq/web3-subprovider": "../ledgerjs/packages/web3-subprovider",
    "@material-ui/core": "^1.3.1",
    "@material-ui/icons": "^1.1.0",
    "babel-polyfill": "^6.26.0",
    "finalhandler": "^1.1.1",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "roboto-fontface": "^0.9.0",
    "web3": "^0.20.6",
    "web3-provider-engine": "^14.0.6"
  },
  "devDependencies": {
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "eth-sig-util": "^2.0.1",
    "ethereumjs-tx": "^1.3.6",
    "ganache-core": "https://github.com/unao/ganache-core.git#f656b6e846b13d6896248ef7468fedd280cd6e7d",
    "memdown": "^3.0.0",
    "mocha": "^5.2.0",
    "parcel-bundler": "~1.8.1",
    "serve-static": "^1.13.2",
    "typeface-megrim": "0.0.54"
  }
}
